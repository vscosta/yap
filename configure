ac_subst_vars='SHELL PATH_SEPARATOR PACKAGE_NAME PACKAGE_TARNAME PACKAGE_VERSION PACKAGE_STRING PACKAGE_BUGREPORT exec_prefix prefix program_transform_name bindir sbindir libexecdir datadir sysconfdir sharedstatedir localstatedir libdir includedir oldincludedir infodir mandir build_alias host_alias target_alias DEFS ECHO_C ECHO_N ECHO_T LIBS CC CFLAGS LDFLAGS CPPFLAGS ac_ct_CC EXEEXT OBJEXT CXX CXXFLAGS ac_ct_CXX AWK GCC C_INTERF_FLAGS C_PARSER_FLAGS build build_cpu build_vendor build_os host host_cpu host_vendor host_os target target_cpu target_vendor target_os CPP EGREP GECODE_VERSION GECODE_EXTRALIBS DefHeapSpace DefStackSpace DefTrailSpace LN_S INSTALL_PROGRAM INSTALL_SCRIPT INSTALL_DATA RANLIB ac_ct_RANLIB INDENT ac_ct_INDENT AR ac_ct_AR MPI_CC ac_ct_MPI_CC INSTALL_INFO REXE ENABLE_GECODE ENABLE_PRISM CPLINT_LIBS CPLINT_CFLAGS CPLINT_LDFLAGS CPLINT_SHLIB_LD SHLIB_SUFFIX ENABLE_CPLINT INSTALL_ENV PRE_INSTALL_ENV ZLIBS ZLIB_TARGETS ZLIB_PLTARGETS ZLIB_INSTALL M4 M4GENHDRS ARCH INSTALL_DLLS MERGE_DLL_OBJS SHLIB_CFLAGS SHLIB_CXXFLAGS SO EXEC_SUFFIX DO_SECOND_LD SHLIB_LD SHLIB_CXX_LD YAPLIB_LD YAPLIB_CFLAGS EXTRA_LIBS_FOR_DLLS EXTRA_LIBS_FOR_SWIDLLS YAPLIB DYNYAPLIB SONAMEFLAG YAP_EXTRAS NO_BUILTIN_REGEXP ENABLE_CHR ENABLE_CLIB ENABLE_CLPQR ENABLE_HTTP ENABLE_JPL ENABLE_PLDOC ENABLE_PLUNIT ENABLE_R ENABLE_RDF ENABLE_SEMWEB ENABLE_SGML ENABLE_ZLIB ENABLE_CLPBN_BP JAVA JAVA_HOME JUNIT JAVAC JAVADOC JAR LIBJPL IN_UNIX YAPMPILIB LAM_MPI_CC MPI_OBJS MPI_LIBS INSTALL_COMMAND INSTALLCLP JAVALIBS JAVACFLAGS LAMOBJS MAX_WORKERS STATIC_MODE ENABLE_WINCONSOLE EXTRA_INCLUDES_FOR_WIN32 ENABLE_CUDD ENABLE_BDDLIB CUDD_LDFLAGS CUDD_CPPFLAGS ENABLE_MINISAT ENABLE_REAL REAL_TARGET REAL_INCLUDES REAL_LIBS INSTALL_MATLAB MATLAB_INCLUDE INSTALL_PRISM M4GENABSMI PROLOG_SYSTEM IN_YAP IN_SWI TARGETS PLTARGETS CHR_TARGETS CLIB_PTHREADS CLIB_TARGETS CLIB_PLTARGETS CLIB_NETLIBS CLIB_CRYPTLIBS RFC2045CHARSET MAILDROP_CFLAGS LIBOBJS LTLIBOBJS'
